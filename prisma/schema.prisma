datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Link {
  id            String   @id @default(cuid())
  code          String   @unique
  url           String
  totalClicks   Int      @default(0)
  lastClickedAt DateTime?
  createdAt     DateTime @default(now())
}